<% content_for :title do %>
  <title>Add an Event | Trip Planner</title>
<% end %>

<%= render 'trips/header' %>

<div class="container">
  <% if @event.errors.any? %>
    <%= render 'validation' %>
  <% end %>
  <div class="row block mid-full">
    <div class="col-xs-12">
      <h1>Add a New Event</h1>
      <hr><br>
      <%= render 'form' %>
    </div>
  </div>
</div>
